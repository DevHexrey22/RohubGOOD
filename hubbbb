		TabOpen.AutoButtonColor = false
		TabOpen.Font = Enum.Font.GothamSemibold
		TabOpen.Text = ("       %s"):format(tabName)
		TabOpen.TextColor3 = (library.tabinfo.button == nil and theme.accent) or theme.accent2
