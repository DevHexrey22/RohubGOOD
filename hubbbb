                BtnC.Name = "BtnC"
                BtnC.Parent = Btn

                Btn.MouseButton1Click:Connect(function()
                    spawn(function()
                        utils:Ripple(Btn)
                    end)
                    spawn(callback)
                end)
            end

            function modules:Label(text)
